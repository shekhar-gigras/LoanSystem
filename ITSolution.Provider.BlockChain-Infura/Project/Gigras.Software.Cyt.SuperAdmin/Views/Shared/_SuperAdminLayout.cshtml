<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Super Admin</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/superadminsite.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Gigras.Software.Cyt.SuperAdmin.styles.css" asp-append-version="true" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css" rel="stylesheet" />
    <link rel="shortcut icon" href="~/favicon.ico" />
    <!--begin::Fonts-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" />
    <!--end::Fonts-->
    <!--begin::Page Vendor Stylesheets(used by this page)-->
    <link href="~/sadmin/plugins/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Page Vendor Stylesheets-->
    <!--begin::Global Stylesheets Bundle(used by all pages)-->
    <link href="~/sadmin/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/sadmin/css/style.bundle.css" rel="stylesheet" type="text/css" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script>var hostUrl = "~/sadmin/assets/";</script>
    <script src="~/sadmin/plugins/global/plugins.bundle.js"></script>
    <script src="~/sadmin/js/scripts.bundle.js"></script>
    <script src="~/sadmin/plugins/custom/datatables/datatables.bundle.js"></script>
    <script src="~/sadmin/js/custom/widgets.js"></script>
    <script src="~/sadmin/js/custom/apps/chat/chat.js"></script>
    <link rel="stylesheet" href="~/css/dynamic-form.css" asp-append-version="true" />
    <script src="https://cdn.jsdelivr.net/npm/web3@latest/dist/web3.min.js"></script>
    <script src="~/js/superadminsite.js" asp-append-version="true"></script>
    <script src="~/js/contract/smart-contract.js" asp-append-version="true"></script>
    <script src="~/js/contract/borrower-smart-contract.js" asp-append-version="true"></script>
    <script src="~/js/contract/lender-contract.js" asp-append-version="true"></script>
    <script src="~/js/admin-website/metamask-init.js"></script>
    <script src="~/js/common/grid.js"></script>

</head>
<body id="kt_body" class="header-fixed header-tablet-and-mobile-fixed toolbar-enabled toolbar-fixed toolbar-tablet-and-mobile-fixed aside-enabled aside-fixed" style="--kt-toolbar-height:55px;--kt-toolbar-height-tablet-and-mobile:55px">
    <!-- Loader element -->
    <div id="loader" style="display:none; position: fixed; top: 50%; left: 50%; transform: translate(-50%, -50%);">
        <div class="spinner-border text-primary" role="status">
            <span class="visually-hidden">Loading...</span>
        </div>
    </div>
    <div class="d-flex flex-column flex-root">
        <!--begin::Page-->
        <div class="page d-flex flex-row flex-column-fluid">
            <!--begin::Aside-->
            @await Html.PartialAsync("~/Views/Partial/SAdmin/_leftnavigation.cshtml")
            <!--end::Aside-->
            <!--begin::Wrapper-->
            <div class="wrapper d-flex flex-column flex-row-fluid" id="kt_wrapper">
                @await Html.PartialAsync("~/Views/Partial/SAdmin/_header.cshtml")
                <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
                    @RenderBody()
                </div>
                @await Html.PartialAsync("~/Views/Partial/SAdmin/_footer.cshtml")
            </div>
        </div>
    </div>
    <!--begin::Scrolltop-->
    <div id="kt_scrolltop" class="scrolltop" data-kt-scrolltop="true">
        <!--begin::Svg Icon | path: icons/duotune/arrows/arr066.svg-->
        <span class="svg-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                <rect opacity="0.5" x="13" y="6" width="13" height="2" rx="1" transform="rotate(90 13 6)" fill="black" />
                <path d="M12.5657 8.56569L16.75 12.75C17.1642 13.1642 17.8358 13.1642 18.25 12.75C18.6642 12.3358 18.6642 11.6642 18.25 11.25L12.7071 5.70711C12.3166 5.31658 11.6834 5.31658 11.2929 5.70711L5.75 11.25C5.33579 11.6642 5.33579 12.3358 5.75 12.75C6.16421 13.1642 6.83579 13.1642 7.25 12.75L11.4343 8.56569C11.7467 8.25327 12.2533 8.25327 12.5657 8.56569Z" fill="black" />
            </svg>
        </span>
        <!--end::Svg Icon-->
    </div>
    <!--end::Scrolltop-->


    @* <script src="~/sadmin/js/custom/apps/user-management/permissions/list.js"></script> *@

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
